{
  "name": "money-tracking-app",
  "version": "0.1.0",
  "description": "A budget app to manage own money",
  "main": "src/js/index.js",
  "scripts": {
    "test": "jest",
    "test:e2e": "jest -c jest-e2e.config.js",
    "test:unit": "jest -c jest-unit.config.js",
    "test:dev": "npm run test -- --watch",
    "dev": "gulp build --dev",
    "prod": "gulp build --prod",
    "prod:debug": "npm run prod -- --debug",
    "postinstall": "husky install",
    "generate-jsdoc": "jsdoc src -r -d docs -c jsdoc.config.json",
    "husky-add-hook": "husky add",
    "preCommit": "lint-staged",
    "start-server": "nodemon server/index.js --watch server"
  },
  "lint-staged": {
    "*.html": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.js": [
      "eslint",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flaviocolonna/money-tracking-app.git"
  },
  "keywords": [
    "budget",
    "app",
    "money"
  ],
  "author": "Flavio Colonna",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/flaviocolonna/money-tracking-app/issues"
  },
  "homepage": "https://github.com/flaviocolonna/money-tracking-app#readme",
  "dependencies": {
    "@fastify/cors": "^8.0.0",
    "axios": "^0.27.2",
    "better-sqlite3": "^7.6.1",
    "core-js": "^3.23.4",
    "fastify": "^4.2.1",
    "quick.db": "^9.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-env": "^7.18.6",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.14",
    "browserify": "^17.0.0",
    "del": "^6.0.0",
    "eslint": "^8.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^3.0.0",
    "gulp-inject": "^5.0.5",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "husky": "^5.1.1",
    "jest": "^28.1.3",
    "jest-puppeteer": "^6.1.1",
    "jsdoc": "^3.6.6",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "puppeteer": "^15.4.0",
    "tui-jsdoc-template": "^1.2.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "yargs": "^16.2.0"
  }
}
